platform :ios, '9.0'

target 'weatherTest' do
  use_frameworks!

  # Networking library
  pod 'Alamofire', '~> 4.1'

  # Parsin library
  pod 'SwiftyJSON', '~> 4.0'
  
  # Database library
  pod 'RealmSwift'
  
  # Keyboard
  pod 'IQKeyboardManagerSwift'
  
  # Progress HUD
  pod 'MBProgressHUD', '~> 1.1.0'

  target 'weatherTestTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
